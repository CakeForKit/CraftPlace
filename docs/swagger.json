    "definitions": {
        "reqresp.AddPostRequest": {
            "type": "object",
            "required": [
                "description",
                "shopID"
            ],
            "properties": {
                "description": {
                    "type": "string",
                    "maxLength": 255,
                    "example": "Магазин сережек"
                },
                "shopID": {
                    "type": "string",
                    "example": "bb2e8400-e29b-41d4-a716-446655442222"
                }
            }
        },
        "reqresp.AddProductRequest": {
            "type": "object",
            "required": [
                "categoryIDs",
                "cost",
                "description",
                "shopID",
                "title"
            ],
            "properties": {
                "categoryIDs": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "cost": {
                    "type": "integer",
                    "minimum": 0,
                    "example": 100
                },
                "description": {
                    "type": "string",
                    "maxLength": 255,
                    "example": "Магазин сережек"
                },
